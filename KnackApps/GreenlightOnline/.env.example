# Greenlight Online API - Environment Variables
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to git!

# Knack API (get from Knack Builder > Settings > API & Code)
KNACK_APP_ID=your_knack_app_id
KNACK_API_KEY=your_knack_api_key

# MailerSend API (get from MailerSend dashboard)
MAILERSEND_API_KEY=your_mailersend_api_key

# Server port (local development)
PORT=3001
